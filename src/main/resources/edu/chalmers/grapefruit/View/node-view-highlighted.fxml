            fx:controller="edu.chalmers.grapefruit.View.NodeView" fx:id="anchorPane">
   <children>
      <Circle fx:id="position" fill="#9ac6ee" layoutX="24.0" layoutY="27.0" radius="20.0"
              stroke="BLACK" strokeType="INSIDE" style="-fx-stroke: #FFFFFF;
